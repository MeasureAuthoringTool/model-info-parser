{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "array-type": [true, "generic"],
    "indent": [
      true,
      "spaces",
      2
    ],
    "no-console": false
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": [
      "dist/**/*",
      "node_modules/**/*"
    ]
  }
}
